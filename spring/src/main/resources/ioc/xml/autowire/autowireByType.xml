<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 配置User对象创建 并且使用set方法注入name属性-->
    <bean id="emp" class="org.bougainvilleas.spring.ioc.xml.autowire.Emp" autowire="byType"></bean>
    <bean id="dept1" class="org.bougainvilleas.spring.ioc.xml.autowire.Dept">
        <property name="name" value="byType注入"></property>
    </bean>

</beans>